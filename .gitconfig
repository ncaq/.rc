[color]
	ui = true
[core]
	autocrlf = false
	editor = emacsclient
	hooksPath = ~/.rc/hooks
	quotePath = false
	symlinks = true
[diff]
	algorithm = histogram
[fetch]
	prune = true
[log]
	date = iso
[pager]
	diff = diff-highlight | less
	log = diff-highlight | less
	show = diff-highlight | less
[pull]
	prune = true
	rebase = false
[push]
	default = current
[user]
	email = ncaq@ncaq.net
	name = ncaq
[github]
	user = ncaq
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[include]
	path = ~/.rc/.gitconfig.local
